  use_frameworks!

  pod 'Firebase/Core'
  pod 'Firebase/Functions'
  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'
  
  
  
  target 'GravitySwitchTests' do
    inherit! :search_paths
    # Pods for testing
    
  end

  target 'GravitySwitchUITests' do
    inherit! :search_paths
    # Pods for testing
    
  end

end
